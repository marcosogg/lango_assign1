<table class="table is-fullwidth">
  <thead>
    <tr>
      <th>Place</th>
      <th>Category</th>
      <th>Description</th>
    </tr>
  </thead>
  <tbody>
    {{#each collection.places}}
    <tr>
      <td>
        {{title}}
      </td>
      <td>
        {{artist}}
      </td>
      <td>
        {{duration}}
      </td>
    </tr>
    {{/each}}
  </tbody>
</table>